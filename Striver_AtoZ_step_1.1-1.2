//22
public:
    void printSquare(int n) {
        // code here
        
        int i,j,k,l;
    
     for(i=n;i>=1;i--)
    {
        for(j=n;j>=i;j--)
            cout<<j<<" ";
        for(k=1;k<=(i-1)*2-1;k++)
            cout<<i<<" ";
        for(l=i;l<=n;l++){
            if(l==1)
                continue;
            cout<<l<<" ";
        }
        cout<<endl;
    }
    for(i=2;i<=n;i++)
    {
        for(j=n;j>=i;j--)
            cout<<j<<" ";
        for(k=1;k<=(i-1)*2-1;k++)
            cout<<i<<" ";
        for(l=i;l<=n;l++)
            cout<<l<<" ";
        cout<<endl;;
    }
    }